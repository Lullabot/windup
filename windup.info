name        = Windup
description = A clean theme for Drupal 7
core        = 7.x

; Remove Drupal core styles.
stylesheets[all][]      = system.theme.css
stylesheets[all][]      = system.menus.css

; Stylesheets
;
; Don't add any external libraries (e.g. normalize.css) here, use bower and
; grunt wiredep (see README.md)

; DO NOT REMOVE OR EDIT BETWEEN THESE LINES
; bower:css
; endbower

; Local Stylesheets
stylesheets[all][] = css/style.css

; JavaScripts
;
; Don't add any external libraries (e.g. enquire, masonry etc) here, use bower
; and grunt wiredep (see README.md)

; DO NOT REMOVE OR EDIT BETWEEN THESE LINES
; bower:js
; endbower

; Local JavaScript
; scripts[] = js/windup.js

regions[header]         = Header
regions[help]           = Help Messages
regions[content]        = Content
regions[footer]         = Footer
